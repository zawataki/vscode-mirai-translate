{
	"name": "mirai-translator",
	"displayName": "Mirai Translator (みらい翻訳)",
	"description": "Translate by high-precision machine translation service named Mirai Translator (みらい翻訳 という高精度な機械翻訳サービスによる翻訳)",
	"version": "1.0.3",
	"publisher": "zawa",
	"license": "SEE LICENSE IN LICENSE.md",
	"engines": {
		"vscode": "^1.41.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/zawataki/vscode-mirai-translator"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.translate"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.translate",
				"title": "Translate from English to Japanese using Mirai Translator"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"@types/request-promise": "^4.1.45",
		"cheerio": "^1.0.0-rc.3",
		"request": "^2.88.0",
		"request-promise": "^4.2.5"
	}
}
